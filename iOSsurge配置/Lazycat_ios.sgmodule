#!name=Lazycat直连与共存

[General]
ipv6 = true
ipv6-vif = always
always-real-ip = %APPEND% *.heiyu.space, *.lazycat.cloud
always-raw-tcp-hosts = %APPEND% *.heiyu.space, *.lazycat.cloud
tun-excluded-routes = %APPEND% 6.6.6.6/32, 2000::6666/128

[Rule]
DOMAIN-SUFFIX,heiyu.space,LC

[Proxy]
LazyCat = socks5, 127.0.0.1, 31086, block-quic=on
LazyCat-http = http, 127.0.0.1, 31085, block-quic=on

[Proxy Group]
LC = url-test, LazyCat, LazyCat-http, no-alert=0, hidden=0, include-all-proxies=
